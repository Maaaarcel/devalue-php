{
  "name": "maaaarcel/devalue-php",
  "type": "library",
  "description": "A library for stringifying and parsing complex data. Inspired by the devalue JavaScript library.",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "https://github.com/Maaaarcel/devalue-php",
  "version": "1.0.1",
  "keywords": [
    "devaulue",
    "json",
    "serialization"
  ],
  "authors": [
    {
      "name": "Marcel Schüßler",
      "homepage": "https://github.com/Maaaarcel"
    }
  ],
  "scripts": {
    "bench": "phpbench run tests/Benchmark --report aggregate",
    "test": "phpunit --configuration tests/Unit/phpunit.xml"
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.28",
    "phpbench/phpbench": "^1.2.7"
  },
  "autoload": {
    "psr-4": {
      "Maaaarcel\\DevaluePhp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maaaarcel\\Tests\\": "tests"
    }
  }
}